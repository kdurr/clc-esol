<h1>Classes</h1>

<table>
  <thead>
    <tr>
      <th>Class</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= link_to "#{course.name}", course_path(course) %></td>
        <td></td>
        <td><%= course.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
